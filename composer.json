{
    "name":              "gyg/generated-hydrator",
    "description":       "Fork of ocramius' Object Hydrator project that allows very fast array to object to array conversion",
    "type":              "library",
    "license":           "MIT",
    "homepage":          "https://github.com/getyourguide/GeneratedHydrator",
    "keywords":          [
        "hydrator",
        "performance",
        "array conversion",
        "object conversion",
        "serializer"
    ],
    "authors": [
        {
            "name":     "Marco Pivetta",
            "email":    "ocramius@gmail.com",
            "homepage": "http://ocramius.github.com/"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.1",
        "nikic/php-parser": "^4.2.1",
        "ocramius/code-generator-utils": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "GeneratedHydrator\\": "src/GeneratedHydrator"
        }
    },
    "config": {
        "sort-packages": true
    }
}
